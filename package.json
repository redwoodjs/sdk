{
  "name": "rw-sdk-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "Redwood SDK monorepo",
  "scripts": {
    "format": "prettier --write ./sdk/src starter/src addons/passkey/src",
    "knip": "knip",
    "check": "pnpm build:sdk && pnpm typecheck:addons && pnpm typecheck:starter",
    "build:sdk": "(cd sdk && pnpm build)",
    "typecheck:addons": "for d in addons/*/; do if [ -f \"$d/tsconfig.json\" ]; then pnpm exec tsc --project \"$d/tsconfig.json\"; fi; done",
    "typecheck:starter": "pnpm exec tsc --project starter/tsconfig.json",
    "test:e2e": "node scripts/test-e2e.mjs",
    "setup:e2e": "pnpm setup:wrangler",
    "setup:wrangler": "./scripts/setup-wrangler-auth.sh",
    "unrelease": "node scripts/unrelease.mjs"
  },
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@10.18.0",
  "devDependencies": {
    "@antfu/eslint-config": "^5.0.0",
    "@manypkg/cli": "^0.25.0",
    "@types/node": "^22.0.0",
    "eslint": "^9.5.0",
    "expect-type": "^1.2.2",
    "knip": "~5.64.0",
    "pnpm": "^10.27.0",
    "prettier": "^3.3.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "tsx": "^4.15.7",
    "typescript": "^5.4.5",
    "vitest": "^3.1.1",
    "wrangler": "^4.35.0"
  },
  "pnpm": {
    "overrides": {
      "@modelcontextprotocol/sdk@1.19.1": "1.25.1",
      "body-parser@2.2.0": "2.2.1",
      "esbuild@0.18.20": "0.25.12",
      "esbuild@0.19.12": "0.25.12",
      "glob@10.4.5": "10.5.0",
      "js-yaml@4.1.0": "4.1.1",
      "mdast-util-to-hast@13.2.0": "13.2.1",
      "qs@6.14.0": "6.14.1",
      "tar-fs@3.1.0": "3.1.1",
      "tar@7.5.1": "7.5.2",
      "tmp@0.2.3": "0.2.5",
      "vite@7.1.9": "7.3.0"
    },
    "onlyBuiltDependencies": [
      "@prisma/client",
      "esbuild",
      "sharp",
      "unrs-resolver",
      "workerd"
    ]
  }
}
