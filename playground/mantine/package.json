{
  "name": "@rwsdk/mantine-playground",
  "version": "1.0.0",
  "description": "A RedwoodSDK starter with Mantine",
  "main": "index.js",
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "vite build",
    "dev": "vite dev",
    "dev:init": "rw-scripts dev-init",
    "preview": "vite preview",
    "worker:run": "rw-scripts worker-run",
    "clean": "npm run clean:vite",
    "clean:vite": "rm -rf ./node_modules/.vite",
    "release": "rw-scripts ensure-deploy-env && npm run clean && npm run build && wrangler deploy",
    "generate": "rw-scripts ensure-env && wrangler types",
    "check": "npm run generate && npm run types",
    "types": "tsc"
  },
  "dependencies": {
    "rwsdk": "workspace:*",
    "react": "19.2.2",
    "react-dom": "19.2.2",
    "react-server-dom-webpack": "19.2.2",
    "@mantine/core": "^7.11.0",
    "@mantine/hooks": "^7.11.0"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "1.16.1",
    "@cloudflare/workers-types": "4.20251202.0",
    "@types/node": "22.18.8",
    "@types/react": "19.1.2",
    "@types/react-dom": "19.1.2",
    "typescript": "5.9.3",
    "vite": "7.2.6",
    "vitest": "^3.1.1",
    "wrangler": "4.52.1",
    "postcss": "^8.4.38",
    "postcss-preset-mantine": "^1.15.0",
    "postcss-simple-vars": "^7.0.1"
  },
  "overrides": {
    "react": "19.2.2",
    "react-dom": "19.2.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp",
      "workerd"
    ]
  }
