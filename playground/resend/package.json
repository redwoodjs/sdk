    "check": "npm run generate && npm run types",
    "types": "tsc"
  },
  "dependencies": {
    "@react-email/components": "^0.5.2",
    "@react-email/render": "^1.2.2",
    "resend": "^6.0.2",
    "rwsdk": "workspace:*",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-server-dom-webpack": "19.2.3"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "1.19.0",
    "@cloudflare/workers-types": "4.20251229.0",
    "@types/node": "22.18.8",
    "@types/react": "19.1.2",
    "@types/react-dom": "19.1.2",
    "typescript": "5.9.3",
    "vite": "7.3.0",
    "vitest": "^3.1.1",
    "wrangler": "4.54.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp",
      "workerd"
