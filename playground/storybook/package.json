{
  "name": "storybook-playground",
  "version": "1.0.0",
  "description": "A bare-bones RedwoodSDK starter",
  "main": "index.js",
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "vite build",
    "dev": "vite dev",
    "dev:init": "rw-scripts dev-init",
    "preview": "vite preview",
    "worker:run": "rw-scripts worker-run",
    "clean": "npm run clean:vite",
    "clean:vite": "rm -rf ./node_modules/.vite",
    "release": "rw-scripts ensure-deploy-env && npm run clean && npm run build && RWSDK_DEPLOY=1 wrangler deploy",
    "generate": "rw-scripts ensure-env && wrangler types",
    "check": "npm run generate && npm run types",
    "types": "tsc",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "rwsdk": "workspace:*",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-server-dom-webpack": "19.2.4"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "1.21.2",
    "@cloudflare/workers-types": "4.20260127.0",
    "@types/node": "~24.10.0",
    "@types/react": "19.2.9",
    "@types/react-dom": "19.2.3",
    "storybook": "^10.0.3",
    "@storybook/react": "^10.0.3",
    "@storybook/react-vite": "^10.0.3",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^10.0.3",
    "@storybook/addon-docs": "^10.0.3",
    "@storybook/addon-a11y": "^10.0.3",
    "@storybook/addon-vitest": "^10.0.3",
    "@storybook/testing-library": "^0.2.2",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "vitest": "^4.0.0",
    "wrangler": "4.60.0"
  },
  "pnpm": {
  }
}
