{
  "name": "@redwoodjs/sdk",
  "version": "1.0.0-alpha.6",
  "description": "Build fast, server-driven webapps on Cloudflare with SSR, RSC, and realtime",
  "type": "module",
  "bin": {
    "rw-scripts": "./bin/rw-scripts.mjs",
    "rwsync": "./bin/rwsync"
  },
  "files": [
    "./README.md",
    "./dist",
    "./bin"
  ],
  "scripts": {
    "build": "tsc --build --clean && tsc",
    "release": "./scripts/release.sh",
    "test": "vitest --run",
    "test:e2e": "vitest --config ./vitest.config.mts --run",
    "debug:sync": "tsx ./src/scripts/debug-sync.mts",
    "smoke-test": "tsx ./src/scripts/smoke-test.mts"
  },
  "exports": {
    "./vite": {
      "default": "./dist/src/vite/index.mjs",
      "types": "./dist/src/vite/index.d.mts"
    },
    "./worker": {
      "react-server": "./dist/src/runtime/entries/worker.js",
      "types": "./dist/src/runtime/entries/worker.d.ts",
      "default": "./dist/src/runtime/entries/react-server-only.js"
    },
    "./client": {
      "react-server": "./dist/src/runtime/entries/no-react-server.js",
      "workerd": "./dist/src/runtime/entries/clientSSR.js",
      "types": "./dist/src/runtime/entries/client.d.ts",
      "default": "./dist/src/runtime/entries/client.js"
    },
    "./__ssr": {
      "react-server": "./dist/src/runtime/entries/no-react-server.js",
      "types": "./dist/src/runtime/entries/ssr.d.ts",
      "default": "./dist/src/runtime/entries/ssr.js"
    },
    "./__ssr_bridge": {
      "react-server": "./dist/src/runtime/entries/no-react-server.js",
      "types": "./dist/src/runtime/ssrBridge.d.ts",
      "default": "./dist/src/runtime/ssrBridge.js"
    },
    "./__vendor_client_barrel": {
      "default": "./dist/__intermediate_builds/rwsdk-vendor-client-barrel.js"
    },
    "./__vendor_server_barrel": {
      "default": "./dist/__intermediate_builds/rwsdk-vendor-server-barrel.js"
    },
    "./router": {
      "types": "./dist/src/runtime/entries/router.d.ts",
      "default": "./dist/src/runtime/entries/router.js"
    },
    "./auth": {
      "types": "./dist/src/runtime/entries/auth.d.ts",
      "default": "./dist/src/runtime/entries/auth.js"
    },
    "./db": {
      "types": "./dist/src/runtime/lib/db/index.d.ts",
      "default": "./dist/src/runtime/lib/db/index.js"
    },
    "./debug": {
      "types": "./dist/src/runtime/lib/debug.d.ts",
      "default": "./dist/src/runtime/lib/debug.js"
    },
    "./constants": {
      "types": "./dist/src/runtime/constants.d.ts",
      "default": "./dist/src/runtime/constants.js"
    },
    "./turnstile": {
      "types": "./dist/src/runtime/lib/turnstile/turnstile.d.ts",
      "default": "./dist/src/runtime/lib/turnstile/turnstile.js"
    },
    "./llms": {
      "types": "./dist/src/llms/index.d.ts",
      "default": "./dist/src/llms/index.js"
    },
    "./realtime/worker": {
      "types": "./dist/src/runtime/lib/realtime/worker.d.ts",
      "default": "./dist/src/runtime/lib/realtime/worker.js"
    },
    "./realtime/client": {
      "types": "./dist/src/runtime/lib/realtime/client.d.ts",
      "default": "./dist/src/runtime/lib/realtime/client.js"
    },
    "./realtime/durableObject": {
      "types": "./dist/src/runtime/lib/realtime/durableObject.d.ts",
      "default": "./dist/src/runtime/lib/realtime/durableObject.js"
    },
    "./e2e/environment": {
      "types": "./dist/src/lib/e2e/environment.d.mts",
      "default": "./dist/src/lib/e2e/environment.mjs"
    },
    "./e2e/dev": {
      "types": "./dist/src/lib/e2e/dev.d.mts",
      "default": "./dist/src/lib/e2e/dev.mjs"
    },
    "./e2e/browser": {
      "types": "./dist/src/lib/e2e/browser.d.mts",
      "default": "./dist/src/lib/e2e/browser.mjs"
    },
    "./e2e/release": {
      "types": "./dist/src/lib/e2e/release.d.mts",
      "default": "./dist/src/lib/e2e/release.mjs"
    }
  },
  "keywords": [
    "rwsdk",
    "redwoodsdk",
    "redwoodjs",
    "redwood",
    "react",
    "server components",
    "sdk",
    "toolkit",
    "starter-kit",
    "typescript",
    "vite",
    "react",
    "jsx",
    "react-server-components",
    "rsc",
    "cloudflare",
    "cloudflare-workers",
    "durable-objects",
    "edge",
    "serverless",
    "webapp",
    "fullstack"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/redwoodjs/sdk",
    "directory": "sdk"
  },
  "author": "RedwoodSDK <peter@redwoodjs.com>",
  "license": "MIT",
  "dependencies": {
    "@ast-grep/napi": "^0.38.5",
    "@cloudflare/workers-types": "^4.20250407.0",
    "@puppeteer/browsers": "^2.8.0",
    "@types/fs-extra": "^11.0.4",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/react-is": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "chokidar": "^3.6.0",
    "debug": "^4.4.0",
    "enhanced-resolve": "^5.18.1",
    "eventsource-parser": "^3.0.0",
    "execa": "^9.5.2",
    "fs-extra": "^11.3.0",
    "glob": "^11.0.1",
    "ignore": "^7.0.4",
    "jsonc-parser": "^3.3.1",
    "kysely": "^0.28.2",
    "kysely-do": "^0.0.1-rc.1",
    "lodash": "^4.17.21",
    "magic-string": "^0.30.17",
    "miniflare": "^4.20250405.0",
    "picocolors": "^1.1.1",
    "proper-lockfile": "^4.1.2",
    "puppeteer-core": "^22.8.1",
    "react-is": "^19.0.0",
    "rsc-html-stream": "^0.0.6",
    "tmp-promise": "^3.0.3",
    "ts-morph": "^25.0.1",
    "unique-names-generator": "^4.7.1",
    "vibe-rules": "^0.2.31",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "peerDependencies": {
    "@cloudflare/vite-plugin": "^1.12.4",
    "react": "19.2.0-canary-3fb190f7-20250908 <20.0.0",
    "react-dom": "19.2.0-canary-3fb190f7-20250908 <20.0.0",
    "react-server-dom-webpack": ">=19.2.0-canary-3fb190f7-20250908 <20.0.0",
    "vite": "^6.2.6 || 7.x",
    "wrangler": "^4.35.0"
  },
  "packageManager": "pnpm@9.14.4+sha512.c8180b3fbe4e4bca02c94234717896b5529740a6cbadf19fa78254270403ea2f27d4e1d46a08a0f56c89b63dc8ebfd3ee53326da720273794e6200fcf0d184ab",
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/js-beautify": "^1.14.3",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.14.0",
    "@types/proper-lockfile": "^4.1.4",
    "js-beautify": "^1.15.4",
    "semver": "^7.7.1",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vitest": "^3.1.1"
  }
}
